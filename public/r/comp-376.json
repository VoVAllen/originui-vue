      "content": "<template>\n  <DropdownMenu>\n    <DropdownMenuTrigger asChild>\n      <Button size=\"icon\" variant=\"outline\" aria-label=\"Open account menu\">\n        <Icon\n          name=\"lucide:circle-user-round\"\n          class=\"size-4\"\n          aria-hidden=\"true\"\n        />\n      </Button>\n    </DropdownMenuTrigger>\n    <DropdownMenuContent class=\"max-w-64\">\n      <DropdownMenuLabel class=\"flex items-start gap-3\">\n        <img\n          src=\"/avatar.jpg\"\n          alt=\"Avatar\"\n          width=\"32\"\n          height=\"32\"\n          class=\"shrink-0 rounded-full\"\n        />\n        <div class=\"flex min-w-0 flex-col\">\n          <span class=\"text-foreground truncate text-sm font-medium\">\n            Keith Kennedy\n          </span>\n          <span class=\"text-muted-foreground truncate text-xs font-normal\">\n            k.kennedy@originui.com\n          </span>\n        </div>\n      </DropdownMenuLabel>\n      <DropdownMenuSeparator />\n      <DropdownMenuGroup>\n        <DropdownMenuItem>\n          <Icon\n            name=\"lucide:bolt\"\n            class=\"size-4 opacity-60\"\n            aria-hidden=\"true\"\n          />\n          <span>Option 1</span>\n        </DropdownMenuItem>\n        <DropdownMenuItem>\n          <Icon\n            name=\"lucide:layers-2\"\n            class=\"size-4 opacity-60\"\n            aria-hidden=\"true\"\n          />\n          <span>Option 2</span>\n        </DropdownMenuItem>\n        <DropdownMenuItem>\n          <Icon\n            name=\"lucide:book-open\"\n            class=\"size-4 opacity-60\"\n            aria-hidden=\"true\"\n          />\n          <span>Option 3</span>\n        </DropdownMenuItem>\n      </DropdownMenuGroup>\n      <DropdownMenuSeparator />\n      <DropdownMenuGroup>\n        <DropdownMenuItem>\n          <Icon\n            name=\"lucide:pin\"\n            class=\"size-4 opacity-60\"\n            aria-hidden=\"true\"\n          />\n          <span>Option 4</span>\n        </DropdownMenuItem>\n        <DropdownMenuItem>\n          <Icon\n            name=\"lucide:user-pen\"\n            class=\"size-4 opacity-60\"\n            aria-hidden=\"true\"\n          />\n          <span>Option 5</span>\n        </DropdownMenuItem>\n      </DropdownMenuGroup>\n      <DropdownMenuSeparator />\n      <DropdownMenuItem>\n        <Icon\n          name=\"lucide:log-out\"\n          class=\"size-4 opacity-60\"\n          aria-hidden=\"true\"\n        />\n        <span>Logout</span>\n      </DropdownMenuItem>\n    </DropdownMenuContent>\n  </DropdownMenu>\n</template>\n",
