{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "comp-260",
  "type": "registry:component",
  "registryDependencies": [
    "https://originui.com/r/button.json",
    "https://originui.com/r/label.json",
    "https://originui.com/r/slider.json"
  ],
  "files": [
    {
      "path": "app/registry/components/comp-260.vue",
      "content": "<script setup lang=\"ts\">\nconst min_price = 5;\nconst max_price = 1240;\nconst value = ref([min_price, max_price]);\n\nconst formatPrice = (price: number) => {\n  return price === max_price\n    ? `$${price.toLocaleString()}+`\n    : `$${price.toLocaleString()}`;\n};\n</script>\n\n<template>\n  <div class=\"*:not-first:mt-3\">\n    <Label class=\"tabular-nums\">\n      From {{ formatPrice(value[0] || min_price) }} to\n      {{ formatPrice(value[1] || max_price) }}\n    </Label>\n    <div class=\"flex items-center gap-4\">\n      <Slider\n        v-model=\"value\"\n        :min=\"min_price\"\n        :max=\"max_price\"\n        aria-label=\"Price range slider\"\n      />\n      <Button variant=\"outline\">Go</Button>\n    </div>\n  </div>\n</template>\n",
      "type": "registry:component"
    }
  ],
  "meta": {
    "tags": [
      "slider",
      "range slider",
      "label",
      "button",
      "radix"
    ]
  }
}